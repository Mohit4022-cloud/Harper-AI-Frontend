# Harper AI - Local Development Environment

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000

# Performance Optimizations
SWC_MINIFY=true
NODE_ENV=development
NEXT_PUBLIC_DEBUG=true

# Authentication
JWT_SECRET=harper-ai-super-secret-jwt-key-for-local-dev-only
JWT_REFRESH_SECRET=harper-ai-refresh-token-secret-for-local-dev
NEXTAUTH_SECRET=harper-ai-nextauth-secret-for-local-development
NEXTAUTH_URL=http://localhost:3000

# AI Services
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id_here
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_CALLER_NUMBER=your_twilio_phone_number_here

# Feature Flags
NEXT_PUBLIC_ENABLE_CALLING=true
NEXT_PUBLIC_ENABLE_RECORDING=true
NEXT_PUBLIC_ENABLE_TRANSCRIPTION=true
NEXT_PUBLIC_ENABLE_AI_COACHING=true
NEXT_PUBLIC_ENABLE_REAL_TIME_TRANSCRIPTION=true

# Development Settings
NEXT_TELEMETRY_DISABLED=1
# Base URL for Twilio webhooks
BASE_URL=https://harper-ai-frontend.onrender.com
